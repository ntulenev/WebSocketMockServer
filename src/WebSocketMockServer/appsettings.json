{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "FileLoaderConfiguration": {
    "Folder": "Files",
    "Mapping": [
      {
        "File": "RequestA.json",
        "Responses": [
          { "File": "Response1.json" },
          {
            "File": "Response2.json",
            "Delay": "5000"
          }
        ]
      }
    ]
  }
}
